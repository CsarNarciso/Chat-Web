#Multiple Compose files configuration
COMPOSE_PATH_SEPARATOR=:
COMPOSE_FILE=docker-compose.yaml:ComposeFiles/postgres-compose.yaml:ComposeFiles/eureka-compose.yaml:ComposeFiles/config-compose.yaml:ComposeFiles/user-compose.yaml
#Postgres
POSTGRES_DB=db
