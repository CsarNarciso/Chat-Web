POSTGRES_PASSWORD=letmein
#User API
SPRING_DATASOURCE_URL=jdbc:postgresql://postgres/db
SPRING_DATASOURCE_USERNAME=postgres
SPRING_DATASOURCE_PASSWORD=letmein
SPRING_JPA_HIBERNATE_DDL_AUTO=create-drop